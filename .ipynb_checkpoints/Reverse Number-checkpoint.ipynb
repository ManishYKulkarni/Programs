    "def reverse_num(num):\n",
    "    rev=0\n",
    "    r=0\n",
    "    while(num>0):\n",
    "        r=num%10\n",
    "        rev=(rev*10)+r\n",
    "        num=num//10\n",
    "    print(rev)\n",
    "num=int(input(\"Enter a number:\"))\n",
    "reverse_num(num)"
   ]
