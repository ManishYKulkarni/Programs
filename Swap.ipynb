    "def is_swap(a,b):\n",
    "    temp=0\n",
    "    print(\"before swap\",a,b)\n",
    "    temp=a\n",
    "    a=b\n",
    "    b=temp\n",
    "    print(\"\"====================================\")\n",
    "    print(\"after swap\",a,b)\n",
    "    \n",
    "a=int(input(\"Enter a number=\"))\n",
    "b=int(input(\"Enter a number\"))\n",
    "is_swap(a,b)"
   ]
